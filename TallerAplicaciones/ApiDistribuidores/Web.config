<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <system.web>
    <compilation debug="true" targetFramework="4.0" />
  </system.web>
  <system.serviceModel>
    <services>
      <service behaviorConfiguration="ApiDistribuidores.ApiDistribuidoresBehavior"
        name="ApiDistribuidores.ApiDistribuidores">
        <endpoint address="" binding="wsHttpBinding" name="ApiDistribuidores.ApiDistribuidoresEndPoint"
          contract="ApiDistribuidores.IApiDistribuidores" />
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost/ApiDistribuidores/" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="ApiDistribuidores.ApiDistribuidoresBehavior">
          <serviceMetadata httpGetEnabled="True"/>
          <serviceDebug includeExceptionDetailInFaults="False" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <bindings>
      <wsHttpBinding>
        <binding name="ApiDistribuidores.ApiDistribuidoresBinding">
          <security/>
        </binding>
      </wsHttpBinding>
    </bindings>
  </system.serviceModel>
 <system.webServer>
    <modules runAllManagedModulesForAllRequests="true"/>
  </system.webServer>
  
</configuration>
